.navbar
	input#showLinks(type="checkbox", name="showLinks")
	.logodiv
		a.logo(href="/") Chat App
		label.hamburger(for="showLinks")
	.links
		.innerLink
			.link
				a(href="/loggedinHome") Home
			.dropdownToggle
				.toggle Services
				.dropdown
					a(href="/loggedinHome") All Contacts
					a(href="/addContact") Add New Contact
